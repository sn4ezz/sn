<html>
  <body>
     <script>
          var user = prompt('Кто пришел?', '')
          if (user == 'Админ') {
                var pass = prompt('Введите пароль', '');
                if (pass == 'Чёрный Властелин') {
                     alert('Добро пожаловать!');
                }
                else if (pass == null) {
                     alert('Вход отменен');
                }
                else {
                     alert('Пароль неверен');
                }
          }      
          else if (user == null) {
                alert('Вход отменен')
                }
          else {
                 alert('Я вас не знаю');
                }
    </script>   
  </body>
</html>  
